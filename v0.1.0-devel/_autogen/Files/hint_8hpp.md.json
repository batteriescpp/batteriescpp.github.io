  "programlisting": "// Copyright 2021-2022 Anthony Paul Astolfi\n//\n#pragma once\n\n#include <batteries/config.hpp>\n//\n\nnamespace batt {\n\n// =============================================================================\n// Branch prediction hints.\n//\n#ifdef BATT_PLATFORM_IS_WINDOWS\n#define BATT_HINT_TRUE(expr) static_cast<bool>(expr)\n#define BATT_HINT_FALSE(expr) static_cast<bool>(expr)\n\n#else\n#define BATT_HINT_TRUE(expr) __builtin_expect(static_cast<bool>(expr), 1)\n#define BATT_HINT_FALSE(expr) __builtin_expect(static_cast<bool>(expr), 0)\n\n#endif\n}  // namespace batt",
