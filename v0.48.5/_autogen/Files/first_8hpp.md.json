  "programlisting": "// Copyright 2021-2022 Anthony Paul Astolfi\n//\n#pragma once\n#ifndef BATTERIES_SEQ_FIRST_HPP\n#define BATTERIES_SEQ_FIRST_HPP\n\n#include <batteries/config.hpp>\n//\n\nnamespace batt {\nnamespace seq {\n\n//==#==========+==+=+=++=+++++++++++-+-+--+----- --- -- -  -  -   -\n// first\n//\n\nstruct FirstBinder {\n};\n\ninline FirstBinder first()\n{\n    return {};\n}\n\ntemplate <typename Seq>\n[[nodiscard]] auto operator|(Seq&& seq, FirstBinder)\n{\n    return seq.peek();\n}\n\n}  // namespace seq\n}  // namespace batt\n\n#endif  // BATTERIES_SEQ_FIRST_HPP",
